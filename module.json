{
	"name": "Liveprofessor",
	"type": "OSC",
	"path": "Plugin Host",
	
	"version": "1.1",
	"description": "Controls Liveprofessor with Chataigne via OSC",
	"url":"https://github.com/ziginfo/Liveprofessor-Chataigne-Module",
	"downloadURL": "https://github.com/ziginfo/Liveprofessor-Chataigne-Module/archive/refs/heads/main.zip",
	
	"hasInput": true,
	"hasOutput": true, 

	"hideDefaultCommands": true,
	"hideDefaultParameters": [
		"colorSendMode",
		"autoRange",
		"autoFeedback",
		"pass_through",
		"input",
		"outputs",
		"useHierarchy",
		"colorSendMode",
		"autoAdd",
		"booleanSendMode"
		],
	
	"defaults": {
	
		"OSC Input": {
			"localPort" : 8001 },
			
		"OSC Outputs": {
			"OSC Output": {
				"local" : false,
				"remotePort" : 8000,
				"listenToFeedback":false } }
	},
	
	"parameters": {
		
		"Fee Spare":{"type":"Integer", "min":0,"max":20}
	},
	
	"scripts": ["liveprofessor.js"],
	
	"values":
  		{ "Number of Snapshots":
		{ "type": "Integer",
			"description":"Set Number of Snapshots",
			"default":12 ,
			"min": 12 },
			
		 "Number of Chains":
		{ "type": "Integer",
			"description":"Set Number of Chains",
			"default":12 ,
			"min": 12 }
			
			
			},
	
	"commands": {
	
	
	"Select SetList" : {
			"menu" : "Snapshots",
			"callback" : "set_setlist",
			"parameters" : {
			
			" Set Song N°": {
					"type": "Integer",
					"default": 1, 
					"min":1,
					"max":20,
					"useForMapping": true }
		}  },
		
	"Recall Snapshot" : {
			"menu" : "Snapshots",
			"callback" : "set_snap",
			"parameters" : {
			
			" Set Snapshot N°": {
					"type": "Integer",
					"default": 1, 
					"min":1,
					"max":100,
					"useForMapping": true }
		}  },
		
	"Prev Snapshot" : {
			"menu" : "Snapshots",
			"callback" : "prev_snap"
		},	
		
	"Next Snapshot" : {
			"menu" : "Snapshots",
			"callback" : "next_snap"
		},
		
	"Update Snapshot" : {
			"menu" : "Snapshots",
			"callback" : "update_snap"
		},
		
	"Prev Cue" : {
			"menu" : "Cues",
			"callback" : "prev_cue"
		},	
		
	"Next Cue" : {
			"menu" : "Cues",
			"callback" : "next_cue"
		},	
		
	"Cue Up" : {
			"menu" : "Cues",
			"callback" : "cue_up"
		},
		
	"Cue Down" : {
			"menu" : "Cues",
			"callback" : "cue_down"
		},
		
	"Recall Cue N°" : {
			"menu" : "Cues",
			"callback" : "set_cue",
			"parameters" : {
			
			" Set SonPart": {
					"type": "Integer",
					"default": 1, 
					"min":1,
					"max":100,
					"useForMapping": true }
		}  },
				
	"Stop All Cues" : {
			"menu" : "Cues",
			"callback" : "stop_cues"
		},
			
	"Add New Chain" : {
			"menu" : "Chain Control",
			"callback" : "add_chain",
		 },
		
		
	"Prev Chain" : {
			"menu" : "Chain Control",
			"callback" : "prev_chain"
		},	
		
	"Next Chain" : {
			"menu" : "Chain Control",
			"callback" : "next_chain"
		},
		
	"Prev Plug-in" : {
			"menu" : "Chain Control",
			"callback" : "prev_plug"
		},	
		
	"Next Plug-in" : {
			"menu" : "Chain Control",
			"callback" : "next_plug"
		},
		
	"Move Up" : {
			"menu" : "Chain Control",
			"callback" : "move_up"
		},
		
	"Move Down" : {
			"menu" : "Chain Control",
			"callback" : "move_down"
		},
		
	"Recall View" : {
			"menu" : "Views",
			"callback" : "recall_view",
			"parameters" : {
			
			" Recall View No": {
					"type": "Integer",
					"default":1, 
					"min":1,
					"max":10,
					"useForMapping": true } }  
		},
		
	"Main View" : {
			"menu" : "Views",
			"callback" : "main_view",
			"parameters" : {
					
			"Select a View": {
					"type": "Enum",
					"options" : {
						"Chains" : "Chains",
						"Active Cuelist" :"LiveCueList",
						"Audio Patch" :"PatchView",
						"Wired" : "WireView" } } } 
		},
		
	"Show Chains View" : {
			"menu" : "Views",
			"callback" : "chains_view"
		},
		
	"Show Navigator" : {
			"menu" : "Views",
			"callback" : "navi_view"
		},
		
	"Plugin Manager" : {
			"menu" : "Views",
			"callback" : "plug_manager"
		},

	"Full Screen" : {
			"menu" : "Views",
			"callback" : "fullscreen"
		},					
		
	"Save" : {
			"menu" : "Main Actions",
			"callback" : "save"
		},	
		
	"Save as.." : {
			"menu" : "Main Actions",
			"callback" : "save_as"
		},
		
	"Close Project" : {
			"menu" : "Main Actions",
			"callback" : "close"
		},
	"Open" : {
			"menu" : "Main Actions",
			"callback" : "open"
		},	
		
	"Quit" : {
			"menu" : "Main Actions",
			"callback" : "quit"
		},
		
	"Recall Workspace" : {
			"menu" : "Main Actions",
			"callback" : "recall_workspace",
			"parameters" : {
			
			" Recall Workspace": {
					"type": "Integer",
					"default":1, 
					"min":1,
					"max":10,
					"useForMapping": true }
		}  },

		
	
		
	
		
	}
}
